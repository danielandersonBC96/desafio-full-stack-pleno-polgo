{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/backend/$1.js" },      
    { "src": "/(.*)", "dest": "/frontend/frontend/build/$1" } 
  ]
}
